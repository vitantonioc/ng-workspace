{
    "name": "tf-workspace",
    "version": "2.0.0",
    "author": "Vitantonio Castronuovo",
    "scripts": {
        "prebuild": "npm run build:tfl-core   &&   npm run build:tfl-toolbox   &&   npm run build:tfl-componentes",
        "build": " ng build --prod --project=tf-base",
        "serve": "lite-server -c bs-config.json",
        "clean": "rimraf dist",
        "build:tfl-core": "ng build --project=tfl-core --prod",
        "watch:tfl-core": "watch \"npm run build:tfl-core\" ./projects/tfl-core/src  --filter=./filter_metadata.js",
        "build:tfl-toolbox": "ng build --project=tfl-toolbox --prod",
        "watch:tfl-toolbox": "watch \"npm run build:tfl-toolbox\" ./projects/tfl-toolbox/src  --filter=./filter_metadata.js",
        "build:tfl-componentes": "ng build --project=tfl-componentes --prod",
        "watch:tfl-componentes": "watch \"npm run build:tfl-componentes\" ./projects/tfl-componentes/src  --filter=./filter_metadata.js",
        "build:tf-base": "ng build --project=tf-base --prod",
        "watch:tf-base": "watch \"npm run build:tf-base\" ./projects/tf-base/src  --filter=./filter_metadata.js",
        "watch_all": "concurrently -n w: npm:watch:* -k",
        "watch_build": "watch \"npm run build\" ./projects  --interval=10 --wait=70 --filter=./filter_metadata.js",
        "watch-lib:core": " npm run watch:tfl-core ",
        "watch-lib:toolbox": "wait-on dist/tfl-core/public_api.d.ts &&    npm run watch:tfl-toolbox   ",
        "watch-lib:componentes": "wait-on dist/tfl-toolbox/public_api.d.ts &&   npm run watch:tfl-componentes  ",
        "watch-lib:base": "wait-on  dist/tfl-componentes/public_api.d.ts &&   npm run watch:tf-base  ",
        "wait-lib:all": "wait-on dist/tf-base/index.html  && ng serve  --liveReload=true --watch=true --port=4200 --prod",
        "version:major": "gulp version:major",
        "version:minor": "gulp version:minor",
        "version:patch": "gulp version:patch",
        "start": "concurrently  \"npm run clean \"  \"run-p watch-lib:*\"  \"npm run wait-lib:all\" ",
        "publish": " bash ./publish.sh",
        "publicar_npm": "npm build && publish",
        "compodoc": "compodoc -p tsconfig.json",
        "compodoc:serve": "compodoc -s --port 9090"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~8.0",
        "@angular/bazel": "^8.0.0",
        "@angular/common": "~8.0",
        "@angular/compiler": "~8.0",
        "@angular/compiler-cli": "~8.0",
        "@angular/core": "~8.0",
        "@angular/forms": "~8.0",
        "@angular/platform-browser": "~8.0",
        "@angular/platform-browser-dynamic": "~8.0",
        "@angular/router": "~8.0",
        "bootswatch": "^4.1.1",
        "classlist.js": "^1.1.20150312",
        "font-awesome": "^4.7.0",
        "rxjs": "~6.5.2",
        "wait-on": "^3.2.0",
        "web-animations-js": "2.3.1",
        "zone.js": "~0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.800.0",
        "@angular-devkit/build-ng-packagr": "~0.800.0",
        "@angular-devkit/build-optimizer": "^0.800.2",
        "@angular/cli": "8.0.1",
        "@angular/compiler-cli": "~8.0",
        "@angular/language-service": "~8.0",
        "@angular/pwa": "^0.800.2",
        "@angular/service-worker": "^8.0.0",
        "@compodoc/compodoc": "^1.1.9",
        "@types/node": "~12.0.4",
        "gulp": "^3.9.1",
        "gulp-replace": "^1.0.0",
        "gulp-util": "^3.0.8",
        "codelyzer": "^5.0.1",
        "concurrently": "^4.1.0",
        "core-js": "^2.5.7",
        "lite-server": "2.5.3",
        "ng-packagr": "^5.3.0",
        "npm-run-all": "^4.1.5",
        "rimraf": "^2.6.3",
        "ts-node": "~8.2.0",
        "tsickle": ">=0.30.0",
        "tslib": "^1.9.3",
        "typescript": "^3.4.5",
        "watch": "^1.0.2"
    },
    "peerDependencies": {
        "concurrently": "^4.1.0"
    }
}
